<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pro.testsource.dao.TestMapper">


    <select id="selectAll" resultType="com.pro.testsource.entity.TestEntity" >
        SELECT
        id,hhh
        FROM
        test
